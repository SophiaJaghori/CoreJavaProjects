package ArrayProjects;

public class TwoSum {
	public static void main(String[] args) {
		int[]arr = new int[10];
		arr[0] = 2;
		arr[1] = 7;
		arr[2] = 11;
		arr[3] = 15;
		twoSumArry(arr, 9);
		
	}
	static void twoSumArry(int[]arr1, int target) {
		for(int i = 0; i< 9; i++) {
			if(arr1[i]+ arr1[i+1] == target) {
				System.out.println(i + " " + (i+1));
			}
		}
		
	}

}
